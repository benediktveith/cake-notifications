{
    "name": "codekanzlei/cake-notifications",
    "description": "CakePHP3 Notifications Plugin",
    "license": "MIT",
    "type": "cakephp-plugin",
    "homepage": "https://github.com/scherersoftware/cake-notifications",
    "require": {
      "php": ">=5.4",
      "cakephp/cakephp": ">=3.1",
      "josegonzalez/cakephp-queuesadilla": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp": ">=3.1",
        "cakephp/cakephp-codesniffer": "dev-master",
        "phpunit/phpunit": "*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Notifications\\": "src",
            "Notifications\\Test\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Notifications\\": "src",
            "Notifications\\Test\\": "tests"
        }
    }
}
